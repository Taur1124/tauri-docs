---
import Layout from '@src/layouts/Layout.astro'
import { l, t, astroI18n } from 'astro-i18n'
import TableOfContents from '@src/components/TableOfContents.astro'
---

<Layout title={t('api.title')}>
  <main class="prose">
    <h1>{t('api.title')}</h1>
    <h2>JavaScript API</h2>
    <TableOfContents path="api/js/1" />
    <h2>Tauri CLI</h2>
    <TableOfContents path="api/cli/1" />
    <h2>Tauri Config</h2>
    <TableOfContents path="api/config/1" />
  </main>
</Layout>
