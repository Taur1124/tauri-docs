---
import BaseLayout from './BaseLayout.astro'
import Footer from '../components/Layout/Footer.astro'

export interface Props {
  title: string
}

const { title } = Astro.props
---

<BaseLayout title={title}>
  <main class="px-8 max-w-[150ch] mx-auto">
    <slot />
  </main>
  <div class="p-8">
    <Footer />
  </div>
</BaseLayout>
